package com.example;

import java.util.Arrays;
import java.util.List;
import java.util.function.Function;
import java.util.stream.Collectors;

public class MainApp {

    public static void main(String[] args) {
        List<String> moneyStrings = Arrays.asList("$10", "$20", "$50");
        Function<String, Integer> convertToInt =
                s -> Integer.parseInt(s.replace("$", ""));
        List<Integer> numbers = moneyStrings.stream()
                .map(convertToInt)
        System.out.println("Danh sách chuỗi: " + moneyStrings);
        System.out.println("Danh sách số nguyên: " + numbers);
    }
}
